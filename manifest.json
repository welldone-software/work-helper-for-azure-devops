{
    "name": "Help Kit for Azure DevOps",
    "version": "2.0",
    "description": "This Chrome extension offers a variety of features for simpler work with Azure DevOps",
    "background": {
        "scripts": [
            "js/utils/time.js",
            "js/utils/chrome.js",
            "js/config.js",
            "js/jobs/azure/helper/index.js",
            "js/jobs/azure/createBugTask.js",
            "js/jobs/azure/moveReadyBacklogItemsToQa.js",
            "js/jobs/azure/moveFixedBugsToQa.js",
            "js/jobs/azure/rerunFailedPrBuilds.js",
            "js/jobs/general/urlReplacer.js",
            "js/jobs/tmetric/logHours/logHours.js",
            "js/jobs/index.js"
        ]
    },
    "permissions": [
        "alarms",
        "contextMenus",
        "storage",
        "tabs",
        "https://*.tmetric.com/*",
        "https://*.visualstudio.com/*",
        "https://dev.azure.com/*"
    ],
    "manifest_version": 2
}